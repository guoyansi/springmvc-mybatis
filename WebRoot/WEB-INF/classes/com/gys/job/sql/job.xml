<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="job">
	<select id="getJobList" parameterType="java.util.Map" resultType="com.gys.job.model.Job">
		SELECT * from tb_job 
		<where>
			<if test="id !=null">
			 	and id = #{id}
			</if>
		</where>
		 
	</select>
	
</mapper>
